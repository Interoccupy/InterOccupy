    ";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";s:5:"child";a:2:{s:0:"";a:6:{s:4:"guid";a:1:{i:0;a:5:{s:4:"data";s:62:"www.facebook.com/notification/fdf048c55207556255477039f7d9d7b6";s:7:"attribs";a:1:{s:0:"";a:1:{s:11:"isPermaLink";s:5:"false";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:5:"title";a:1:{i:0;a:5:{s:4:"data";s:88:" We don&#039;t support H.R. 2606, the NYC Natural Gas Supply Enhancement Act, because...";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:4:"link";a:1:{i:0;a:5:{s:4:"data";s:105:"http://www.facebook.com/photo.php?fbid=388255811254626&set=a.380505532029654.91112.380489552031252&type=1";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:11:"description";a:1:{i:0;a:5:{s:4:"data";s:1559:"We don&#039;t support H.R. 2606, the NYC Natural Gas Supply Enhancement Act, because we support rebuilding the Rockaways with a sustainable and renewable energy future.<br /> <br /> Call the Whitehouse TODAY: 202-456-1111<br /> <br /> This Act permits construction of a pipeline through the already crumbling infrastructure of the Rockaways, through a wetlands habitat and Gateway National Park. We are troubled by the possible future ramifications of this decision to turn over portions of a National Park to a large corporation without public comment allowed.<br /> <br /> This pipeline will carry fracked gas from Pennsylvania to NYC and along with it, radioactive radon gas. Radon is the leading cause of lung cancer in non smokers and Con-Ed plans on piping this directly into your home for cooking and heating! <br /> <br /> Congress approved this bill last week. Today it is sitting on newly elected President Obama&#039;s desk. Don&#039;t let him think for a minute that the people who live here approve of the construction of this dangerous &amp; toxic pipeline in the middle of a disaster zone. <br /> <br /> Call the Whitehouse TODAY: 202-456-1111<br /> <br /> Say NO to HR 2606, Say NO! to Fracking. <br /> <br /> Green Power!<br/><br/><a href="http://www.facebook.com/photo.php?fbid=388255811254626&amp;set=a.380505532029654.91112.380489552031252&amp;type=1&amp;relevant_count=1" id="" title="" target="" onclick="" style=""><img class="img" src="http://photos-c.ak.fbcdn.net/hphotos-ak-ash4/224162_388255811254626_1805031451_s.jpg" alt="" /></a>";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:7:"pubDate";a:1:{i:0;a:5:{s:4:"data";s:31:"Mon, 19 Nov 2012 16:22:34 +0000";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:6:"author";a:1:{i:0;a:5:{s:4:"data";s:23:"Occupy Sandy Relief NYC";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}}s:32:"http://purl.org/dc/elements/1.1/";a:1:{s:7:"creator";a:1:{i:0;a:5:{s:4:"data";s:23:"Occupy Sandy Relief NYC";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}}}}i:25;a:6:{s:4:"data";s:54:"
      
      
      
      
      
      
      
